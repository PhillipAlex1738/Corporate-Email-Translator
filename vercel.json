{
  "version": 2,
  "builds": [
    {
      "src": "production-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "production-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}